<PasswordResetEntry>:
    orientation: 'vertical'
    spacing: "12dp"
    size_hint_y: None
    font_size: "100dp"
    
    MDTextField:
        hint_text: 'email'
        mode: 'rectangle'
        id: passa
        on_text: app.password(self.text)
        password: True
        size_hint_y: None
        adaptive_height: True