                        MDCard:
                            opacity: 0.9
                            radius: [5]
                            size_hint_y: None
                            height: 150
                            
                            elevation: 0
                            orientation: 'vertical'
                            spacing: "20dp"
                            md_bg_color: 255/255, 255/255, 255/255, 0.8
                            MDBoxLayout:
                                orientation: 'vertical'
                                padding: [20,0,20,10]
                                MDLabel:
                                    text: "email"
                                    valign: 'top'
                                    font_size: "15sp"
                                MDTextField:
                                    id: email
                                    hint_text: "email"
                                    on_text: app.signin_mail(self.text)
                                    
