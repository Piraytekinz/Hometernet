<MyProducts>:
   
    

    MDBoxLayout:
        orientation: 'vertical'
        md_bg_color: 1,1,1,1
        MDBoxLayout:
            size_hint_y: None
            height: "70dp"
            md_bg_color: 1,1,1,1
            id: prop_title
            padding: ["10dp",0,0,0]
            

            MDLabel:
                text: "My Properties"
                font_size: '20dp'
                bold: True
                theme_font_style: 'Overline'
                
                
            MDIconButton:
                icon: 'trash-can-outline'
                icon_size: '20dp'
                theme_text_color: 'Custom'
                text_color: "black"
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                on_press: 
                    app.play()
                    self.text_color=app.theme_cls.primary_color
                on_release: 
                    layout.delete_user_auth()
                    app.clear_bk() if layout.has_deleted == True else app.do()
                    self.text_color="black"
                
                id: arrow_right
                id: trash
                disabled_color: 0,0,0,1
                

            MDIconButton:
                icon: 'refresh'
                icon_size: '20dp'
                theme_text_color: 'Custom'
                text_color: "black"
                on_press: 
                    app.play()
                    self.text_color=app.theme_cls.primary_color
                    
                on_release: 
                    self.text_color="black"
                    app.reload_products()
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                id: reload
                
                
                disabled_color: 0,0,0,1
                
                

            MDIconButton:
                icon: 'arrow-right'
                icon_size: '20dp'
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                on_press: app.play()
                on_release: app.switch_home()
                id: arrow_right
        MDBoxLayout:
            size_hint_y: None
            height: "50dp"
            
            md_bg_color: 1,1,1,0.5
            
            AnchorLayout:
                anchor_x: 'left'
                anchor_y: 'center'
                
                
                MDIconButton:
                    icon: "account-arrow-left-outline"
                    icon_size: '22dp'
                    on_press: app.play()
                    theme_text_color: 'Custom'
                    text_color: "black"
                    on_press: self.text_color=app.theme_cls.primary_color
                    on_release: self.text_color="black"
                    on_release: app.sign_out_auth()
                    pos_hint: {'center_y': 0.5, 'center_x': 0.5}
                    id: sign_out
                    disabled_color: 0,0,0,1
            AnchorLayout:
                anchor_x: 'center'
                anchor_y: 'center'
                # md_bg_color: 1,0,0,1
                MDIconButton:
                    icon: "account-switch-outline"
                    icon_size: '22dp'
                    on_press: app.play()
                    theme_text_color: 'Custom'
                    text_color: "black"
                    on_press: self.text_color=app.theme_cls.primary_color
                    on_release: self.text_color="black"
                    on_release: app.switch_choice()
                    pos_hint: {'center_y': 0.5, 'center_x': 0.5}
                    id: switch
                    disabled_color: 0,0,0,1
            AnchorLayout:
                anchor_x: 'right'
                anchor_y: 'center'
                MDIconButton:
                    icon: "home-plus-outline"
                    icon_size: '22dp'
                    on_press: app.play()
                    theme_text_color: 'Custom'
                    text_color: "black"
                    on_press: self.text_color=app.theme_cls.primary_color
                    on_release: self.text_color="black"
                    on_release: app.switch_saleorrent()
                    pos_hint: {'center_y': 0.5, 'center_x': 0.9}
                    id: sell
        
        
        MDSeparator:
            color: 0.9,0.9,0.9,0.4

        
            
        Scroller:

            scroll_distance: 1
            scroll_wheel_distance: 50
            smooth_scroll_end: 30
            on_scroll_move: app.reload_products() if self.scroll_y > 1.05 and app.thread_prod_started == False else app.do()
            id: scroller
            MDBoxLayout:
                size_hint_y: None
                adaptive_height: True
                orientation: 'vertical'
                id: relay
                
            
                

            
        
                

                PropertyCardsLayout:
                    size_hint_y: None
                    adaptive_height: True
                    spacing: "20dp"
                    orientation: 'vertical'
                    padding: ["10dp","10dp","10dp","5dp"]
                    id: layout
                    


                
                MDSpinner:
                    active: False
                    size_hint: None, None
                    width: '30dp'
                    height: '30dp'
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    id: spinno
                    opacity: 0
                        

                    
        
            
        

        
           

            

        

                

            
                        
