            Reload:
                
                size_hint: None, None
                height: "46dp"
                width: "46dp"
                radius: ["23dp"]
                pos_hint: {'center_x': 0.5}
                padding: ["23dp"]
                ripple_behavior: True
                id: reload
                elevation: 0
                on_press: app.play()
                on_press: app.reload_products()
                pos_hint: {'center_x': 0.9, 'center_y': 0.2}
