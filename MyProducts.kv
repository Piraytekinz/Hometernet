<MyProducts>:
    MDFloatLayout:
        RelativeLayout:
            MDBoxLayout:
                orientation: 'vertical'
                PropTitle:
                    size_hint_y: None
                    height: arrow_right.height + 55
                    
                    MDIconButton:
                        icon: 'arrow-left'
                        user_font_size: "16dp"
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        on_press: app.switch_home()
                        id: arrow_right

                    MDLabel:
                        text: "My Properties"
                        font_size: '17dp'
                        bold: True
                        halign: 'left'

                    
                
                            
                

                
                    
                ScrollView:
                    MDBoxLayout:
                        orientation: 'vertical'
                        spacing: '30dp'
                        size_hint_y: None
                        adaptive_height: True
                        padding: [5,15,5,5]
                        MDBoxLayout:
                            size_hint_y: None
                            adaptive_height: True
                            spacing: "20dp"
                            SignOut:
                                ripple_behavior: True
                                elevation: 0
                                on_press: app.sign_out_auth()
                                md_bg_color: app.theme_cls.primary_color
                                radius: [10, ]
                                size_hint_y: None
                                height: 50
                                MDLabel:
                                    text: "sign-out"
                                    halign: 'center'
                                    font_size: "12dp"
                                    bold: True
                                    color: 'white'

                            SwitchAccount:
                                ripple_behavior: True
                                on_press: app.switch_account_auth()
                                elevation: 0
                                radius: [10, ]
                                size_hint_y: None
                                height: 50
                                MDLabel:
                                    text: "switch account"
                                    halign: 'center'
                                    font_size: "12dp"
                                    bold: True

                    
                        

                    
                
                    
                        PropertyCardsLayout:
                            size_hint_y: None
                            adaptive_height: True
                            spacing: "20dp"
                            orientation: 'vertical'
                            
            
            MDFloatingActionButton:
                icon: 'plus'
                on_press: app.switch_saleorrent()
                pos_hint: {'center_x': 0.85, 'center_y': 0.1}
                            
<PropTitle@MDCard+CommonElevationBehavior>
<SignOut@MDCard+CommonElevationBehavior>
<SwitchAccount@MDCard+CommonElevationBehavior>
        

               

            
            
            
                
                
            

        