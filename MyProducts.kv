    MDFloatLayout:
        RelativeLayout:
            

            MDBoxLayout:
                orientation: 'vertical'
                # spacing: "10dp"
                PropTitle:
                    size_hint_y: None
                    height: "70dp"
                    # padding: [10]
                    id: prop_title

                    MDIconButton:
                        icon: 'trash-can-outline'
                        icon_size: "20dp"
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        on_press: app.play()
                        on_release: layout.delete_user_auth()
                        id: arrow_right

                    MDLabel:
                        text: "My Properties"
                        font_size: '16dp'
                        bold: True
                        theme_font_style: 'Overline'
                        halign: 'center'

                    
                    MDIconButton:
                        icon: 'arrow-right'
                        icon_size: "20dp"
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        on_press: app.play()
                        on_release: app.switch_home()
                        id: arrow_right
                MDBoxLayout:
                    size_hint_y: None
                    adaptive_height: True
                    # spacing: (Window.size[0]/3)+sell.width*1.5
                    md_bg_color: 1,1,1,0.5
                    
                    AnchorLayout:
                        anchor_x: 'left'
                        anchor_y: 'center'
                        size_hint_y: None
                        height: switch.height
                        # md_bg_color: 1,0,0,1
                        MDIconButton:
                            icon: "account-arrow-left-outline"
                            icon_size: "22dp"
                            on_press: app.play()
                            theme_text_color: 'Custom'
                            text_color: "black"
                            on_press: self.text_color=app.theme_cls.primary_color
                            on_release: self.text_color="black"
                            on_release: layout.sign_out_auth()
                            pos_hint: {'center_y': 0.5, 'center_x': 0.5}
                            id: sign_out
                    AnchorLayout:
                        anchor_x: 'center'
                        anchor_y: 'center'
                        # md_bg_color: 1,0,0,1
                        MDIconButton:
                            icon: "account-switch-outline"
                            icon_size: "22dp"
                            on_press: app.play()
                            theme_text_color: 'Custom'
                            text_color: "black"
                            on_press: self.text_color=app.theme_cls.primary_color
                            on_release: self.text_color="black"
                            on_release: app.switch_choice()
                            pos_hint: {'center_y': 0.5, 'center_x': 0.5}
                            id: switch
                    AnchorLayout:
                        anchor_x: 'right'
                        anchor_y: 'center'
                        MDIconButton:
                            icon: "home-plus-outline"
                            icon_size: "22dp"
                            on_press: app.play()
                            theme_text_color: 'Custom'
                            text_color: "black"
                            on_press: self.text_color=app.theme_cls.primary_color
                            on_release: self.text_color="black"
                            on_release: app.switch_saleorrent()
                            pos_hint: {'center_y': 0.5, 'center_x': 0.9}
                            id: sell
                
                
                

                
                    
