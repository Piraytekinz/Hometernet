<Menu@ButtonBehavior+BoxLayout>:
    size_hint: None, None
    height: menu.height * 2
    width: self.height
    background_color: 1,1,1,1
    
    padding: [self.width/3.9,self.height/3.2,self.width/3.9,self.height/3.5]
    canvas.before:
        Color: 
            rgba: (1,1,1,1)
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [dp(3),]
    
   
    MDIcon:
        icon: 'menu'
        font_size: '21dp'
        
        size_hint: None, None
        adaptive_size: True
        id: menu
        
        
        


