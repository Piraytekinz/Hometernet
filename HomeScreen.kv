<HomeScreen>:
    
    
    
    
    
    ScrollView:
        effect_cls: 'ScrollEffect'
        
        
        scroll_wheel_distance: 50
        smooth_scroll_end: 30
        scroll_timeout: 250
        scroll_distance: 0.5
        id: scroller
        do_scroll_x: False
        on_scroll_move: app.scroll_more()
        
        
        MDBoxLayout:
            id: main_box
            orientation: 'vertical'
            size_hint_y: None
            adaptive_height: True   
            spacing: "47dp"        
            
            
            padding: [0,0,0,10]
            RelativeLayout:
                id: hero_box
                size_hint_y: None
                height: Window.size[1]

                FitImage:
                    id: main_image
                    source: "Apartment-Buildings.jpg"
                    
                MDBoxLayout:
                    
                    padding: ["15dp", "15dp", "15dp", "0dp"]
                    orientation: 'vertical'
                    spacing: "40dp"
                    
                    AnchorLayout:
                        anchor_x: 'left'
                        anchor_y: 'top'
                        
                        padding: [0,0,0,40]
                        size_hint_y: .5
                        MyCard:
                            size_hint: None, None
                            height: menu.height * 2
                            width: self.height
                            ripple_behavior: True
                            radius: ["3dp",]
                            on_press: app.play()
                            on_release: nav.set_state("open")
                            
                            padding: [self.width/3.9,self.height/3.2,self.width/3.9,self.height/3.2]
                            id: menu_card
                            MDIcon:
                                icon: "menu"
                                id: menu
                                font_size: '21dp'
                                size_hint: None, None
                                adaptive_size: True
                                
                                
                                
                                
                                
                                
                                
                                
                                
                                

                                
                            
                        
                    
                    
                        
                        
                    MDLabel:
                        id: src
                        text: "Buy and sell on Hometernet."
                        color: "white"
                        markup: True
                        font_size: "25dp" if Window.size[1] > Window.size[0] else "24dp"
                        halign: 'center'
                        size_hint_y: None
                        adaptive_height: True
                        pos_hint: {'center_y': 0.5, 'center_x': 0.5}
                        bold: True
                        padding_x: "5dp"

                        
                        
                            
                    
                    SearchButton:
                        elevation: 0
                        
                        
                        md_bg_color: 1,1,1,1
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        size_hint_y: None
                        height: '58dp' if Window.size[1] > Window.size[0] else '50dp'
                        
                        size_hint_max_x: dp(100)
                        radius: ['30dp']
                        
                        on_press: app.play()
                        on_release: app.switch_search()
                        id: btn
                        ripple_behavior: True
                        padding: ["30dp", "0dp", "30dp", "0dp"]
                       
                        spacing: "10dp"
                        MDIcon:
                            icon: 'magnify'
                            font_size: '17dp'
                            size_hint: None, None
                            adaptive_size: True
                            pos_hint: {'center_y': 0.5}
                            bold: True

                        MDLabel:
                            text: 'Search'
                            bold: True
                            font_size: '17dp'
                            
                                
                                
                        
                        
                        
                        
                        
                        
                        
                        
                        
                        
                        
                        
                        
                        
                        
                                
                        
                        
                        
                        
                        
                        
                        
                            
                            
                            
                            
                                
                            
                            
                            
                            
                            
                            
                            
                            
                            
                            
                            
                            
                            
                            
                            
                            
                            
                            
                            
                            
                            
                            
                        
                            
                    MDLabel:
                        text: "Bridging the Gap between Worlds."
                        pos_hint: {"center_x": 0.5, "center_y": .7}
                        color: "white"
                        bold: True
                        font_size: "15dp" if Window.size[1] > Window.size[0] else '14dp'
                        halign: 'center'
                        
                        
            
            
            
            
            
            
            
                
            
            MDLabel:
                text: "Best Property Deals. \n \n[size=15dp][color=#9A9A9A]Browse properties from across the globe.[/color][/size]"
                color: "black"
                size_hint_y: None
                font_size: "22dp"
                
                markup: True
                adaptive_height: True
                id: best
                padding_x: "18dp"
                
            
            
            
            
            
            
            
                    

            
            
            
            
            
            
                
                
            
           
            MDBoxLayout:
                height: '130dp'
                size_hint: None, None
                adaptive_width: True
                pos_hint: {'center_x': 0.5}
                padding: ["18dp"]
                spacing: "30dp"
               
                MDFillRoundFlatButton:
                    ripple_effect: False
                    ripple_behavior: False
                    text: "     Sale     "
                    theme_text_color: "Custom"
                    text_color: "white" if grid.op_choice=="Sale" else "black"
                    md_bg_color: app.theme_cls.primary_color if grid.op_choice=="Sale" else [0.9,0.9,0.9,0.7]
                    id: sale_btn
                    ripple_behavior: False
                    on_press: app.regulate() if app.has_pressed == False and grid.starter == False else app.do()
                    pos_hint: {'center_y': .2} if grid.op_choice=="Sale" else {'center_y': .35}
                    ripple_scale: 0
                    font_size: '15dp'
                    size_hint_x: None
                    width: '200dp'
                        
                        
                    
                        
                        
                        

                MDFillRoundFlatButton:
                    ripple_effect: False
                    ripple_behavior: False
                    text: "     Rent     "
                    theme_text_color: "Custom"
                    text_color: "white" if grid.op_choice=="Rent" else "black"
                    md_bg_color: app.theme_cls.primary_color if grid.op_choice=="Rent" else [0.9,0.9,0.9,0.7]
                    id: rent_btn
                    ripple_behavior: False
                    pos_hint: {'center_y': .2} if grid.op_choice=="Rent" else {'center_y': .35}
                    on_press: app.rent_regulate() if app.has_pressed == False and grid.starter == False else app.do()
                    ripple_scale: 0
                    font_size: '15dp'
                    size_hint_x: None
                    width: '200dp'
                        
                    
                        
                        
                    
                
                
                    
            
            
            
            
            
            
            
            
            
            
            
            
                
                
            
            
            
            
            
            
            
            
                
            
            
            
            
            
            
            
            
            
            
            HomeCardsLayout:
                pos_hint: {'center_x': 0.5}
                cols: 1 if Window.size[1] > Window.size[0] else 2
                id: grid
                size_hint: None, None
                adaptive_width: True
                height: self.minimum_height
                spacing: "25dp"
                padding: ["10dp", "0dp", "10dp", "10dp"]
                
                
            MDBoxLayout:
                height: '130dp'
                size_hint: None, None
                adaptive_width: True
                pos_hint: {'center_x': 0.5}
                padding: ["18dp"]
                spacing: "20dp"

                MDIconButton: 
                    icon: 'arrow-left'
                    icon_size: '25dp'
                    on_press: app.play()
                    md_bg_color: 1,1,1,1
                    pos_hint: {'center_x': 0.5}
                    disabled_color: 0,0,0,1
                    on_release: app.thread_reload_sale('reload') if grid.op_choice == "Sale" and len(grid.last_one) > 1 or grid.op_choice == "Rent" and len(grid.last_rent_one) > 1 else app.do()
                    theme_text_color: 'Custom'
                    text_color: app.theme_cls.primary_color
                    id: refresh_sale
                    
                    line_width: 1.1
                    line_color: app.theme_cls.primary_color


                MDIconButton: 
                    icon: 'arrow-right'
                    icon_size: '25dp'
                    on_press: app.play()
                    md_bg_color: 1,1,1,1
                    pos_hint: {'center_x': 0.5}
                    disabled_color: 0,0,0,1
                    on_release: app.thread_reload_sale('more') if grid.starter == False else app.do()
                    line_width: 1.1
                    line_color: 0,0,0,1
                    
                    
                    id: sale_arrow
                        





                    
                        
                    
                    
                    
                    
                    
             
                            
                            
                            
                            
                            
                            
                                
                            
                            
                            
                            
                            
                            
                            
                            
                            
                                        
                                
                        
                        
                        
                        
                        
                    

                    
                    
                    
                    
                    
                    
                    
                    
                    
                            
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    

                    

                    
                    
                            
                    
                            
                
                    
                
                
                
                
                
                        
                
                
                
                

                
                
                
                
                
                        
                
                
                                
                
                
                
                
                
                            
    Nav:
        id: nav
        orientation: 'vertical'
        radius: ["0dp", "10dp","10dp","0dp"]  
        elevation: 0
        padding: [0]
        opening_time: 0.2
        closing_time: 0.1

        # spacing: "20dp"
        
            
        
        
        
        
        
        
        
        
                
        
                
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        MDIconButton:
            icon: 'close'
            icon_size: '21dp'
            on_press: app.play()
            on_release: nav.set_state('close')
            padding_x: ['5dp']
            size_hint: None, None
            adaptive_size: True
            pos_hint: {'center_x': 0.1}
            
        
            
        
        ScrollView:
            bar_width: 0
            effect_cls: "ScrollEffect"
            smooth_scroll_end: 30
            scroll_distance: 1
            do_scroll_x: False

            MDList:     
                padding: ["5dp", "0dp", "5dp", "0dp"]
                
                # GridLayout:
            
                #     size_hint_y: None
                #     adaptive_height: True
                #     spacing: '10dp'
                #     padding: ['10dp', '0dp', '20dp', '10dp']
                #     cols: 2
                #     rows: 1
                    
                    
                    
                    
                    
                    
                
                MDLabel:
                    text: 'Main [color=#000000]Menu[/color]'
                    color: app.theme_cls.primary_color
                    bold: True
                    font_size: '25dp'
                    size_hint_y: None
                    adaptive_height: True
                    # id: mail_text
                    padding_x: '20dp'
                    markup: True
                    on_pos: self.pos = tuple(map(int, args[1]))
                    
                    text_size: self.size
                    padding_y: '25dp'

                MDLabel:
                    text: 'Main'
                    color: 'gray'
                    size_hint_y: None
                    adaptive_height: True
                    font_size: '15dp'
                    padding_x: '20dp'
                    on_pos: self.pos = tuple(map(int, args[1]))
                    text_size: self.size

                OneLineIconListItem:
                    text: "[size=16dp]My Account[/size]"
                    divider: None
                    on_press: app.play()
                    on_release: app.switch_acc()
                    radius: [10]
                    on_pos: self.pos = tuple(map(int, args[1]))
                    IconLeftWidget:
                        icon: 'account-outline'         
                        
                        icon_size: '20dp'
                        pos_hint:{'center_y': 0.5}
                        
                                    
                OneLineIconListItem:
                    text: "[size=16dp]Sell your Property[/size]"
                    divider: None
                    on_press: app.play()
                    on_release: app.switch_products()
                    radius: [10]
                    on_pos: self.pos = tuple(map(int, args[1]))
                    IconLeftWidget:
                        icon: 'bed-outline'         
                        
                        icon_size: '20dp'
                        pos_hint:{'center_y': 0.5}
                OneLineIconListItem:
                    text: "[size=16dp]Bookmarks[/size]"
                    radius: [10]
                    on_press: app.play()
                    on_release: app.switch_bookmarks()
                    on_pos: self.pos = tuple(map(int, args[1]))
                    IconLeftWidget:
                        icon: 'flag-variant-outline'
                        
                        icon_size: '20dp'
                        pos_hint:{'center_y': 0.5}


            
            
                Label:
                    text: 'Others'
                    color: 'gray'
                    size_hint_y: None
                    text_size: self.size
                    height: '45dp'
                    font_size: '15dp'
                    on_pos: self.pos = tuple(map(int, args[1]))
                    valign: 'bottom'
                    padding_x: '20dp'
              
                    

                OneLineIconListItem:
                    text: "[size=16dp]App Tutorial[/size]"
                    divider: None
                    on_press: app.play()
                    on_release: app.switch_tutorial()
                    radius: [10]
                    on_pos: self.pos = tuple(map(int, args[1]))
                    IconLeftWidget:
                        icon: 'book-outline'
                        icon_size: '20dp'
                        pos_hint:{'center_y': 0.5}
                        

                OneLineIconListItem:
                    text: "[size=16dp]About us[/size]"
                    divider: None
                    on_press: app.play()
                    on_release: app.switch_about()
                    radius: [10]
                    on_pos: self.pos = tuple(map(int, args[1]))
                    IconLeftWidget:
                        icon: 'map-legend'
                        icon_size: '20dp'
                        pos_hint:{'center_y': 0.5}
                        
                
                OneLineIconListItem:
                    text: "[size=16dp]Contact us[/size]"
                    divider: None
                    on_press: app.play()
                    on_release: app.switch_contact()
                    radius: [10]
                    on_pos: self.pos = tuple(map(int, args[1]))
                    IconLeftWidget:
                        icon: 'phone-outline'
                        icon_size: '20dp'
                        pos_hint:{'center_y': 0.5}
                    

                OneLineIconListItem:
                    text: "[size=16dp]Meet The Creator[/size]"
                    divider: None
                    on_press: app.play()
                    on_release: app.switch_creator()
                    radius: [10]
                    on_pos: self.pos = tuple(map(int, args[1]))
                    IconLeftWidget:
                        icon: 'pirate'
                        icon_size: '20dp'
                        pos_hint:{'center_y': 0.5}
                        
            
            
            
            
            
            
            
                    
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
                        
            
            
            
            
            
            

            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            

            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            

            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            

