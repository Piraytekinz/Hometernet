                    line_width: 1.1
                    line_color: 0,0,0,1
                    
                    
                    id: sale_arrow
                    disabled_color: 0,0,0,1
                        





                    
                        
                    
                    
                    
                    
                    
             
                            
                            
                            
                            
                            
                            
                                
                            
                            
                            
                            
                            
                            
                            
                            
                            
                                        
                                
                        
                        
                        
                        
                        
                    

                    
                    
                    
                    
                    
                    
                    
                    
                    
                            
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    

                    

                    
                    
                            
                    
                            
                
                    
                
                
                
                
                
                        
                
                
                
                

                
                
                
                
                
                        
                
                
                                
