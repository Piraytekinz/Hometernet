<MyProducts>:
    MDFloatLayout:
        RelativeLayout:
            MDBoxLayout:
                orientation: 'vertical'
                PropTitle:
                    size_hint_y: None
                    height: arrow_right.height + 55
                    padding: [10]
                    id: prop_title

                    MDLabel:
                        text: "My Properties"
                        font_size: '17dp'
                        bold: True
                        theme_font_style: 'Overline'

                    MDIconButton:
                        icon: 'arrow-right'
                        user_font_size: "16dp"
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        on_press: app.switch_home()
                        id: arrow_right
                
                            
                

                
                    
                ScrollView:
                    MDBoxLayout:
                        orientation: 'vertical'
                        spacing: '30dp'
                        size_hint_y: None
                        adaptive_height: True
                        padding: [5,15,5,5]
                        MDBoxLayout:
                            size_hint_y: None
                            adaptive_height: True
                            spacing: "20dp"
                            SignOut:
                                ripple_behavior: True
                                elevation: 0
                                # on_press: app.sign_out_auth()
                                md_bg_color: app.theme_cls.primary_color
                                radius: [10, ]
                                size_hint_y: None
                                height: prop_title.height
                                MDLabel:
                                    text: "sign-out"
                                    halign: 'center'
                                    font_size: "17dp"
                                    bold: True
                                    color: 'white'
                                    id: sign_out

                            SwitchAccount:
                                ripple_behavior: True
                                # on_press: app.switch_account_auth()
                                elevation: 0
                                radius: [10, ]
                                size_hint_y: None
                                height: prop_title.height
                                MDLabel:
                                    text: "switch account"
                                    halign: 'center'
                                    font_size: "17dp"
                                    bold: True
                                    id: switch_account

                    
                        

                    
                
                    
                        MDBoxLayout:
                            size_hint_y: None
                            adaptive_height: True
                            spacing: "20dp"
                            orientation: 'vertical'
                            padding: [10]
                            SaleCard:
                                elevation: 1
                                padding: [0]
                                size_hint: None, None
                                width: Window.size[0] - 200 if Window.size[0] < Window.size[1] else Window.size[0] / 2 + 40
                                pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                                height: Window.size[1] / 2 + 20 if Window.size[1] > Window.size[0] else Window.size[1] - 30
                                id: main_card
                                # on_press: app.editscreen(root.image, root.local_image, root.tot, root.price, root.country, root.town, root.street, root.bedrooms, root.bathrooms, root.landspace, root.email, root.key, root.description, root.amenities)
                                radius: [5, ]
                                MDBoxLayout:
                                    orientation: 'vertical'
                                    padding: [10,]
                                    spacing: "20dp"
                                    SalePic:
                                        
                                        elevation: 1
                                        
                                        radius: [5,]
                                        ripple_behavior: True
                                        RelativeLayout:
                                            FitImage:
                                                source: '3d-electric-car-building.jpg'
                                                radius: [5, ]

                                            
                                            MDBoxLayout:
                                                size_hint: None, None
                                                adaptive_size: True
                                                md_bg_color: 1,1,1,1
                                                padding: [10]
                                                pos_hint: {'center_x': 0.75, 'center_y': 0.89}
                                                MDLabel:
                                                    text: '$780/mth'
                                                    halign: 'center'
                                                    font_size: "14dp"
                                                    md_bg_color: 1,1,1,1
                                                    font_color: "black"
                                                    bold: True
                                                    size_hint: None, None
                                                    adaptive_size: True
                                                    
                                                    
                                                    

                                    
                                        
                                        
                                    MDBoxLayout:
                                        orientation: 'vertical'
                                        spacing: "10dp"
                                        padding: [10, 0, 5, 0]
                                        size_hint_y: None
                                        adaptive_height: True
                                        MDLabel:
                                            text: 'Awesome House'
                                            bold: True
                                            size_hint: None, None
                                            adaptive_size: True
                                            font_size: '15dp'
                                        MDBoxLayout:
                                            size_hint_y: None
                                            adaptive_height: True
                                            
                                            MDLabel:
                                                text: 'Sri Lanka' + ',' + ' ' + 'johi' + ',' + ' ' + 'johi st.'
                                                color: "gray"
                                                font_size: "13dp"
                                                size_hint_y: None
                                                adaptive_height: True
                                        

                                    MDSeparator:

                                    MDBoxLayout:
                                        padding: [10, 5, 0, 17]
                                        size_hint_y: None
                                        adaptive_height: True
                                        MDBoxLayout:
                                            orientation: 'vertical'
                                            spacing: "6dp"
                                            size_hint_y: None
                                            adaptive_height: True
                                            MDLabel:
                                                text: '6'
                                                font_size: "13dp"
                                                bold: True
                                                halign: 'center'
                                                size_hint_y: None
                                                adaptive_height: True
                                            MDLabel:
                                                text: "bedrooms"
                                                color: "black"
                                                font_size: "13dp"
                                                bold: True
                                                halign: 'center'
                                                size_hint_y: None
                                                adaptive_height: True
                                        MDBoxLayout:
                                            orientation: 'vertical'
                                            spacing: "6dp"
                                            size_hint_y: None
                                            adaptive_height: True
                                            MDLabel:
                                                text: '3'
                                                font_size: "13dp"
                                                bold: True
                                                halign: 'center'
                                                size_hint_y: None
                                                adaptive_height: True
                                            MDLabel:
                                                text: "bathrooms"
                                                color: "black"
                                                font_size: "13dp"
                                                bold: True
                                                halign: 'center'
                                                size_hint_y: None
                                                adaptive_height: True
                                        MDBoxLayout:
                                            orientation: 'vertical'
                                            spacing: "6dp"
                                            size_hint_y: None
                                            adaptive_height: True
                                            MDLabel:
                                                text: '675sq.ft'
                                                font_size: "13dp"
                                                bold: True
                                                halign: 'center'
                                                size_hint_y: None
                                                adaptive_height: True
                                            MDLabel:
                                                text: "Land space"
                                                color: "black"
                                                font_size: "13dp"
                                                bold: True
                                                halign: 'center'
                                                size_hint_y: None
                                                adaptive_height: True





                            
            
            MDFloatingActionButton:
                icon: 'plus'
                # on_press: app.switch_saleorrent()
                pos_hint: {'center_x': 0.85, 'center_y': 0.1}

<SalePic@MDCard+CommonElevationBehavior>
<SaleCard@MDCard+CommonElevationBehavior>
                            
<PropTitle@MDCard+CommonElevationBehavior>
<SignOut@MDCard+CommonElevationBehavior>
<SwitchAccount@MDCard+CommonElevationBehavior>
        

               

            
            
            
                
                
            

        