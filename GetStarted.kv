<GetStarted>:
    RecycleView:
        MDBoxLayout:
            md_bg_color: 1,1,1,1
            padding: ['0dp', '10dp', '0dp', '0dp']
            orientation: 'vertical'
            size_hint_y: None
            adaptive_height: True
            spacing: "30dp"
            md_bg_color: 0,0,0.2,1
            MDBoxLayout:
                size_hint_y: None
                height: '80dp'
                md_bg_color: 0,0,0.2,1
                padding: ['25dp', '10dp', '25dp', '30dp']
                MDLabel:
                    text: 'Advertise'
                    color: 'white'


                MDIconButton:
                    icon: 'close'
                    on_press: app.switch_home()
                    pos_hint: {'center_y': 0.5}
                    theme_text_color: 'Custom'
                    text_color: 'white'

            

            MDBoxLayout:
                padding: ['25dp','50dp','25dp', '50dp']
                size_hint_y: None
                adaptive_height: True
                spacing: '30dp'
                orientation: 'vertical'
                md_bg_color: 0,0,0.2,1
                MDBoxLayout:
                    orientation: 'vertical'
                    size_hint_y: None
                    adaptive_height: True
                    spacing: '30dp'
                    MDLabel:
                        text: 'Selling?'
                        font_size: '40dp'
                        bold: True
                        size_hint_y: None
                        adaptive_height: True
                        color: 'white'
                    MDLabel:
                        text: 'We provide you with services to sell your property where everyone can see them at absolutely no cost. We accept various forms of social media such as whatsapp, messenger and twitter for communication with your buyers. Click get started to continue.'
                        font_size: '15dp'
                        color: 'white'
                        size_hint_y: None
                        adaptive_height: True
                    
                    MDTextButton:
                        color: app.theme_cls.primary_color
                        text: 'Privacy Policy'
                        on_press: app.show_privacy()
                
                MDCard:
                    radius: ['20dp']
                    size_hint: None, None
                    width: '200dp'
                    height: '70dp'
                    line_color: 0.9,0.9,0.9,0.8
                    spacing: '10dp'
                    padding: ['20dp']
                    ripple_behavior: True
                    on_press: app.play()
                    on_release: app.switch_sale() if app.curr['email'] != '' else app.switch_signin()
                    MDLabel:
                        text: 'Get Started'
                        font_size: '18dp'
                        bold: True
                        color: 'black'
                    MDIcon:
                        icon: 'arrow-right'
                        icon_size: '20dp'
                        pos_hint: {'center_y': 0.5}
                        bold: True
                        size_hint_x: None
                        width: '20dp'

            
            

            MDBoxLayout:
                padding: ['25dp','70dp','25dp', '50dp']
                size_hint_y: None
                adaptive_height: True
                spacing: '30dp'
                orientation: 'vertical'
                radius: ['30dp', '0dp', '0dp', '0dp']
                md_bg_color: 1,1,1,1
                MDBoxLayout:
                    orientation: 'vertical'
                    size_hint_y: None
                    adaptive_height: True
                    spacing: '30dp'
                    padding: ['0dp', '50dp', '0dp', '0dp']
                    MDLabel:
                        text: 'Renting?'
                        font_size: '40dp'
                        bold: True
                        size_hint_y: None
                        adaptive_height: True
                        color: 'black'
                    MDLabel:
                        text: 'We provide you with services to rent your property where everyone can see them at absolutely no cost. We accept various forms of social media such as whatsapp, messenger and twitter for communication with your buyers as well as a web link if any. Click get started to continue.'
                        font_size: '15dp'
                        color: 'black'
                        size_hint_y: None
                        adaptive_height: True
                    
                    MDTextButton:
                        color: app.theme_cls.primary_color
                        text: 'Privacy Policy'
                        on_press: app.show_privacy()
                
                MDCard:
                    radius: ['20dp']
                    size_hint: None, None
                    width: '200dp'
                    height: '70dp'
                    line_color: 0.9,0.9,0.9,0.8
                    line_width: 1
                    spacing: '10dp'
                    padding: ['20dp']
                    ripple_behavior: True
                    on_press: app.play()
                    on_release: app.switch_rent() if app.curr['email'] != '' else app.switch_signin()
                    md_bg_color: 0,0,0,0
                    MDLabel:
                        text: 'Get Started'
                        font_size: '18dp'
                        bold: True
                        color: 'black'
                    MDIcon:
                        icon: 'arrow-right'
                        icon_size: '20dp'
                        pos_hint: {'center_y': 0.5}
                        bold: True
                        size_hint_x: None
                        width: '20dp'
                        
                        
            
