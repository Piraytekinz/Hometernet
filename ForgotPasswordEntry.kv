<ForgotPasswordEntry>:
    orientation: 'vertical'
    spacing: "12dp"
    size_hint_y: None
    height: "100dp"
    
    MDTextField:
        hint_text: 'email'
        mode: 'rectangle'
        id: emaily
        on_text: app.email(self.text)
        input_type: "mail"
        size_hint_y: None
        adaptive_height: True